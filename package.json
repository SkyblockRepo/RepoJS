{
	"name": "skyblockrepo",
	"version": "0.0.1",
	"description": "NPM package to interface with skyblock repo data",
	"browser": false,
	"engines": {
		"node": ">=22.0.0"
	},
	"packageManager": "pnpm@10.15.0",
	"homepage": "https://skyblockrepo.com/docs/integrations/nodejs",
	"bugs": {
		"url": "https://github.com/SkyblockRepo/RepoJS/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SkyblockRepo/RepoJS.git"
	},
	"files": [
		"dist/**/*"
	],
	"keywords": [
		"skyblockrepo",
		"skyblock",
		"hypixel",
		"hypixel-skyblock"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"license": "MIT",
	"author": "Kaeso",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"test": "vitest run",
		"build": "tsc",
		"format": "biome check --write",
		"lint": "biome lint",
		"check": "biome check"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@types/adm-zip": "^0.5.7",
		"@types/node": "^24.3.1",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"adm-zip": "^0.5.16",
		"env-paths": "^3.0.0",
		"zod": "^4.1.12"
	}
}
